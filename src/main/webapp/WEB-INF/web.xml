<web-app>
  <welcome-file-list>
  	<welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>bookcreate</servlet-name>
  	<servlet-class>com.github.bookstore.Add</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>bookcreate</servlet-name>
  	<url-pattern>/addbook</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>viewbook</servlet-name>
  	<servlet-class>com.github.bookstore.Show</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>viewbook</servlet-name>
  	<url-pattern>/showallbooks</url-pattern>
  </servlet-mapping>
</web-app>
